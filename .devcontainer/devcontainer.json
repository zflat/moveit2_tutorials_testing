{
  "name": "moveit2_tutorials",
  "image": "moveit2_tutorials:latest",
  "runArgs": [
    "--privileged",
    "--net=host",
    "-v=/tmp/.X11-unix:/tmp/.X11-unix",
    "--env=DISPLAY",
    "--env=QT_X11_NO_MITSHM=1",
    "--env=ROS_DOMAIN_ID=25",
    "--env=CONTAINER_WS_FOLDER=${containerWorkspaceFolder}",
    "--env=RMW_IMPLEMENTATION=rmw_cyclonedds_cpp",
  ],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "remoteUser": "guest",
}
